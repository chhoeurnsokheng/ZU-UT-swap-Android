                        android:alpha="0.35"
                        app:cardBackgroundColor="@color/black_333333"
                        app:cardCornerRadius="@dimen/dimen_100"
                        app:cardElevation="@dimen/dimen_0"
                        app:strokeColor="@color/gray"
                        app:strokeWidth="@dimen/dimen_2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="0"
                            android:textAppearance="@style/medium_28"
                            android:textColor="@color/white" />
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_12"
                    android:orientation="vertical">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/number_3"
                        android:layout_width="@dimen/dimen_82"
                        android:layout_height="@dimen/dimen_82"
                        android:layout_marginBottom="@dimen/dimen_14"
                        android:alpha="0.35"
                        app:cardBackgroundColor="@color/black_333333"
                        app:cardCornerRadius="@dimen/dimen_100"
                        app:cardElevation="@dimen/dimen_0"
                        app:strokeColor="@color/gray"
                        app:strokeWidth="@dimen/dimen_2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="3"
                            android:textAppearance="@style/medium_28"
                            android:textColor="@color/white" />
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/number_6"
                        android:layout_width="@dimen/dimen_82"
                        android:layout_height="@dimen/dimen_82"
                        android:layout_marginBottom="@dimen/dimen_14"
                        android:alpha="0.35"
                        app:cardBackgroundColor="@color/black_333333"
                        app:cardCornerRadius="@dimen/dimen_100"
                        app:cardElevation="@dimen/dimen_0"
                        app:strokeColor="@color/gray"
                        app:strokeWidth="@dimen/dimen_2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="6"
                            android:textAppearance="@style/medium_28"
                            android:textColor="@color/white" />
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/number_9"
                        android:layout_width="@dimen/dimen_82"
                        android:layout_height="@dimen/dimen_82"
                        android:layout_marginBottom="@dimen/dimen_14"
                        android:alpha="0.35"
                        app:cardBackgroundColor="@color/black_333333"
                        app:cardCornerRadius="@dimen/dimen_100"
                        app:cardElevation="@dimen/dimen_0"
                        app:strokeColor="@color/gray"
                        app:strokeWidth="@dimen/dimen_2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="9"
                            android:textAppearance="@style/medium_28"
                            android:textColor="@color/white" />
                    </com.google.android.material.card.MaterialCardView>

                    <LinearLayout
                        android:layout_width="@dimen/dimen_82"
                        android:layout_height="@dimen/dimen_82"
                        android:layout_marginBottom="@dimen/dimen_14"
                        android:orientation="vertical"
                        android:gravity="center">
                        <ImageView
                            android:layout_width="@dimen/dimen_28"
                            android:layout_height="@dimen/dimen_28"
                            android:src="@drawable/ic_back_box"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen_2"
                            app:tint="@color/black_222222" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>
</layout>
