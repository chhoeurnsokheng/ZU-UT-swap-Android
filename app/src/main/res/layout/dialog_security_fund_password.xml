<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingBottom="@dimen/dimen_50">

            <ImageView
                android:layout_width="@dimen/dimen_38"
                android:layout_height="@dimen/dimen_38"
                android:layout_marginBottom="@dimen/dimen_20"
                android:src="@drawable/ic_fund_key_normal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_22"
                android:text="Enter Fund Password"
                android:textAppearance="@style/medium_18"
                android:textColor="@color/color_main" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_50"
                android:orientation="horizontal">

                <View
                    android:layout_width="@dimen/dimen_12"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_marginHorizontal="@dimen/dimen_5"
                    android:background="@drawable/bg_circular_border"
                    android:backgroundTint="@color/color_main" />

                <View
                    android:layout_width="@dimen/dimen_12"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_marginHorizontal="@dimen/dimen_5"
                    android:background="@drawable/bg_circular_border"
                    android:backgroundTint="@color/color_main" />

                <View
                    android:layout_width="@dimen/dimen_12"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_marginHorizontal="@dimen/dimen_5"
                    android:background="@drawable/bg_circular_border"
                    android:backgroundTint="@color/color_main" />

                <View
                    android:layout_width="@dimen/dimen_12"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_marginHorizontal="@dimen/dimen_5"
                    android:background="@drawable/bg_circular_border"
                    android:backgroundTint="@color/color_main" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_12"
                    android:orientation="vertical">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/number_1"
                        android:layout_width="@dimen/dimen_82"
                        android:layout_height="@dimen/dimen_82"
                        android:layout_marginBottom="@dimen/dimen_14"
                        android:alpha="0.35"
                        app:cardBackgroundColor="@color/black_333333"
                        app:cardCornerRadius="@dimen/dimen_100"
                        app:cardElevation="@dimen/dimen_0"
                        app:strokeColor="@color/gray"
                        app:strokeWidth="@dimen/dimen_2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="1"
                            android:textAppearance="@style/medium_28"
                            android:textColor="@color/white" />
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/number_4"
                        android:layout_width="@dimen/dimen_82"
                        android:layout_height="@dimen/dimen_82"
                        android:layout_marginBottom="@dimen/dimen_14"
                        android:alpha="0.35"
                        app:cardBackgroundColor="@color/black_333333"
                        app:cardCornerRadius="@dimen/dimen_100"
                        app:cardElevation="@dimen/dimen_0"
                        app:strokeColor="@color/gray"
                        app:strokeWidth="@dimen/dimen_2">

